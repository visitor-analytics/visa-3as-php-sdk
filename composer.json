{
    "name": "visitor-analytics/3as-sdk",
    "description": "PHP SDK for 3AS Companies",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Visa\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Oliviu Sivu",
            "email": "os@visitor-analytics.io"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "nesbot/carbon": "2.62.1",
        "lcobucci/jwt": "4.3.0",
        "guzzlehttp/guzzle": "7.5.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "laminas/laminas-hydrator": "4.4.0",
        "respect/validation": "^2.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.16",
        "ramsey/conventional-commits": "^1.3.1",
        "marcocesarato/php-conventional-changelog": "^1.16.0"
    },
    "scripts": {
        "changelog": "conventional-changelog",
        "release": "conventional-changelog --commit",
        "release:patch": "conventional-changelog --patch --commit",
        "release:minor": "conventional-changelog --minor --commit",
        "release:major": "conventional-changelog --major --commit"
    },
    "version": "1.0.2"
}